<h1>Listing docs</h1>

<table>
  <tr>
    <th>Body</th>
    <th>Source</th>
    <th>Sourcedb</th>
    <th>Sourceid</th>
    <th>Serial</th>
    <th>Section</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @docs.each do |doc| %>
  <tr>
    <td><%= doc.body %></td>
    <td><%= doc.source %></td>
    <td><%= doc.sourcedb %></td>
    <td><%= doc.sourceid %></td>
    <td><%= doc.serial %></td>
    <td><%= doc.section %></td>
    <td><%= link_to 'Show', pmdoc_path(doc.sourceid) %></td>
  </tr>
<% end %>
</table>

