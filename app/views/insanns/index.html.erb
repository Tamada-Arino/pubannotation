<h1>Instance annotations</h1>

<table class="ann">
  <tr>
    <th>Annset</th>
    <th>Hid</th>
    <th>Type of instantiation</th>
    <th>Object of instantiation</th>
  </tr>

<% @insanns.each do |insann| %>
  <tr>
    <td><%= insann.annset_name %></td>
    <td><%= insann.hid %></td>
    <td><%= insann.instype %></td>
    <td><%= insann.insobj_hid %></td>
  </tr>
<% end %>
</table>
