{
  "id": "<%= doc.id -%>",
  "text": "<%= doc.body.gsub(/[\r\n]/,"") -%>",
  "source_db": "<%= doc.sourcedb -%>",
  "source_id": "<%= doc.sourceid -%>",
  <% if doc.has_divs? -%>
    "div_id": "<%= doc.serial -%>",
  <% end -%>
  "section": "<%= doc.section -%>",
  "source_url": "<%= doc.source -%>"
}<%= ',' if @docs.present? && doc_counter < @docs.size-1 -%>
