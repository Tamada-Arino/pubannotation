<h1>Listing relanns</h1>

<table>
  <tr>
    <th>Annset</th>
    <th>Hid</th>
    <th>Subject</th>
    <th>Relation</th>
    <th>Object</th>
  </tr>

<% @relanns.each do |relann| %>
  <tr>
    <td><%= relann.annset_name %></td>
    <td><%= relann.hid %></td>
    <td><%= relann.subject_hid %></td>
    <td><%= relann.relation %></td>
    <td><%= relann.object_hid %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Relann', new_relann_path %>
