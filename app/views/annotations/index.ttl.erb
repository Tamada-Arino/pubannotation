@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sio: <http://semanticscience.org/ontology/sio.owl> .
@prefix pubann: <http://pubannotation.org/ontology/pubann.owl> .
@prefix ge: <http://bionlp-st.dbcls.jp/ge/ontology/bionlp-st-ge.owl> .

<% @catanns.each do |ca| %>
S<%= ca[:id][1..-1] %>
	rdf:type sio:text_span;
	ao:fromSourceDocument <<%= @docuri %>>;
	pubann:via_proxy_text <<%= @texturi %>>;
	pubann:begin_at_caret_offset <%= ca[:span][:begin] %>;
	pubann:end_at_caret_offset <%= ca[:span][:end] %>.
<%= ca[:id] %>
	rdf:type pubann:entity_in_context;
	sio:denote ge:<%= ca[:category] %>;
	pubann:in_context S<%= ca[:id][1..-1] %>.
<% end %>

<% @insanns.each do |ia| %>
<%= ia[:id] %>
	rdf:type pubann:entity_in_text_context;
	sio:denote ge:<%= @catidx[ia[:object]] %>;
	pubann:in_context S<%= ia[:object][1..-1] %>.
<% end %>

<% @relanns.each do |ra| %>
<% unless ra[:type] == 'coreferenceOf' or ra[:type] == 'equivalentTo' %>
<%= ra[:subject] %> ge:<%= ra[:type] %> <%= ra[:object] %>.
<% end %>
<% end %>
