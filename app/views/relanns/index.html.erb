<h1>Listing relanns</h1>

<table>
  <tr>
    <th>Hid</th>
    <th>Subject</th>
    <th>Relation</th>
    <th>Object</th>
    <th>Annset</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @relanns.each do |relann| %>
  <tr>
    <td><%= relann.hid %></td>
    <td><%= relann.subject.hid %></td>
    <td><%= relann.relation %></td>
    <td><%= relann.object.hid %></td>
    <td><%= relann.annset.name %></td>
    <td><%= link_to 'Show', relann %></td>
    <td><%= link_to 'Edit', edit_relann_path(relann) %></td>
    <td><%= link_to 'Destroy', relann, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Relann', new_relann_path %>
